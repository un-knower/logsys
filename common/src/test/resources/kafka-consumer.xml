<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <property>
        <name>kafka-consumer.bootstrap.servers</name>
        <value>${kafka.bootstrap.servers}</value>
    </property>
    <property>
        <name>kafka-consumer.group.id</name>
        <value>test</value>
    </property>
    <property>
        <name>kafka-consumer.enable.auto.commit</name>
        <value>true</value>
    </property>
    <property>
        <name>kafka-consumer.auto.offset.reset</name>
        <value>earliest</value>
    </property>
    <property>
        <name>kafka-consumer.key.deserializer</name>
        <value>org.apache.kafka.common.serialization.ByteArrayDeserializer</value>
    </property>
    <property>
        <name>kafka-consumer.value.deserializer</name>
        <value>org.apache.kafka.common.serialization.ByteArrayDeserializer</value>
    </property>
</configuration>