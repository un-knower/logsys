<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <property>
        <name>kafka-consumer.zookeeper.connect</name>
        <value>${prop.kafka.zookeeper.connect}</value>
    </property>
    <property>
        <name>kafka-consumer.group.id</name>
        <value>forest</value>
    </property>
    <property>
        <name>kafka-consumer.enable.auto.commit</name>
        <value>true</value>
    </property>
    <property>
        <name>kafka-consumer.auto.commit.interval.ms</name>
        <value>1000</value>
    </property>
    <property>
        <name>kafka-consumer.auto.offset.reset</name>
        <value>smallest</value>
    </property>
    <property>
        <name>kafka-consumer.key.deserializer</name>
        <value>org.apache.kafka.common.serialization.ByteArrayDeserializer</value>
    </property>
    <property>
        <name>kafka-consumer.key.value</name>
        <value>org.apache.kafka.common.serialization.ByteArrayDeserializer</value>
    </property>
</configuration>